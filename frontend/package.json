{
  "name": "front-end",
  "version": "1.0.0",
  "scripts": {
    "start": "npx parcel serve src/index.html",
    "build-server": "npx parcel build src/index.html --dist-dir ../backend/static --public-url /static",
    "build-local": "npx parcel build src/index.html --dist-dir ../backend/static --no-content-hash --no-optimize --public-url /static",
    "watch-server": "nodemon --watch src --exec \"npm run build-server\"",
    "watch-local": "nodemon --watch src --exec \"npm run build-local\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "nodemon": "^3.1.4",
    "phaser": "^3.80.1",
    "phaser3-rex-plugins": "^1.80.5"
  },
  "devDependencies": {
    "parcel": "^2.12.0",
    "process": "^0.11.10"
  }
}
